{
  "name": "parse-ical",
  "version": "1.2.1",
  "description": "Parse iCAL (.ics) data into JS objects",
  "author": "Janina Wibker",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JaninaWibker/parse-ical.git"
  },
  "keywords": [
    "ical",
    "ics",
    "calendar",
    "typescript"
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "rrule": "^2.8.1"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts"
}
